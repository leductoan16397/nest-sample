{
  "name": "cdn-admin-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint-staged": "lint-staged --allow-empty",
    "test": "jest",
    "test:report": "jest --ci --reporters=default --reporters=jest-junit --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@mongodb-js/zstd": "2.0.1",
    "@nestjs/cache-manager": "2.3.0",
    "@nestjs/common": "11.0.12",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "11.0.12",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/microservices": "11.0.12",
    "@nestjs/mongoose": "11.0.2",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/swagger": "11.0.7",
    "@nestjs/throttler": "6.4.0",
    "@types/multer": "1.4.12",
    "amqp-connection-manager": "4.1.14",
    "amqplib": "0.10.5",
    "axios": "1.8.4",
    "bcryptjs": "3.0.2",
    "cache-manager": "5.7.6",
    "cache-manager-redis-yet": "5.1.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "express-basic-auth": "1.2.1",
    "ioredis": "5.6.0",
    "moment-timezone": "0.5.47",
    "mongoose": "8.12.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "swagger-ui-express": "5.0.1",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.23.0",
    "@nestjs/cli": "11.0.5",
    "@nestjs/testing": "11.0.12",
    "@types/amqplib": "0.10.7",
    "@types/bcrypt": "5.0.2",
    "@types/bcryptjs": "3.0.0",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.11",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "source-map-support": "0.5.21",
    "supertest": "7.1.0",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0"
  },
  "jest-junit": {
    "suiteName": "Unit Test",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{title}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "addFileAttribute": "true",
    "includeConsoleOutput": true,
    "includeShortConsoleOutput": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{mjs,js,ts,jsx,tsx}": "npx eslint ",
    "**/*.{json,md,yml}": "npx prettier --check"
  },
  "packageManager": "yarn@1.22.22"
}
